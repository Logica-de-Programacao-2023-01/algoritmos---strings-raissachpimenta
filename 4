package main

import (
	"fmt"
	"strings"
)

func main() {
	var stringX, stringY string
	fmt.Print("Digite a primeira string: ")
	fmt.Scanln(&stringX)
	fmt.Print("Digite a segunda string: ")
	fmt.Scanln(&stringY)

	if strings.EqualFold(stringX, stringY) {
		fmt.Println("As strings são iguais.")
	} else {
		fmt.Println("As strings são diferentes.")
	}
}
